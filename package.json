  "scripts": {
    "start": "node server.js",
    "client": "PORT=3000 react-scripts start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm install && npm run build:client",
    "build:client": "react-scripts build",
    "build:server": "npm run build:client",
    "start:prod": "NODE_ENV=production node server.js",
    "server": "nodemon server.js",
    "test": "react-scripts test",
    "test:e2e": "jest --config=jest.config.js --detectOpenHandles --runInBand",
    "test:e2e:ci": "cross-env CI=true jest --config=jest.config.js --runInBand --forceExit --detectOpenHandles",
    "test:e2e:setup": "node jest-environment-puppeteer/setup.js",
    "test:e2e:teardown": "node jest-environment-puppeteer/teardown.js",
    "test:e2e:full": "npm run test:e2e:setup && npm run test:e2e && npm run test:e2e:teardown",
    "eject": "craco eject",
    "serve": "node server.js",
    "prod": "npm run build && node server.js",
    "simple-server": "node dev-server.js",
    "test-connection": "node test-connection.js",
    "start-expo": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "data:import": "node server/config/seed.js",
    "data:destroy": "node server/config/seed.js -d",
    "lint:server": "eslint --fix server/**/*.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
